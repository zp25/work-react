{
  "name": "work-react",
  "version": "0.0.0",
  "description": "React starter kit",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.config.js --progress",
    "prebuild:quiet": "npm run clean",
    "build:quiet": "cross-env NODE_ENV=production webpack --env.quiet -p --config webpack.config.js --json > stats.json",
    "analyze": "webpack-bundle-analyzer -p 3001 stats.json dist/",
    "bundle-buddy": "bundle-buddy dist/scripts/*.map",
    "serve": "serve -l 8081"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zp25/work-react.git"
  },
  "author": "zp25",
  "license": "MIT",
