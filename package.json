{
  "name": "work-react",
  "version": "0.0.0",
  "description": "Work React",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --env.development --progress",
    "vendor": "webpack --config vendor.webpack.config.js --progress",
    "dist": "cross-env NODE_ENV=production webpack --env.production -p --progress",
    "serve": "node server.js",
    "serve:dist": "npm run dist && npm run serve",
    "clean": "rimraf dist && npm run vendor",
    "copyindex": "node copyindex",
    "test": "mocha --harmony --check-leaks"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zp25/work-react.git"
  },
