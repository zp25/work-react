{
  "name": "work-react",
  "version": "0.0.0",
  "description": "React starter kit",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p --config webpack.prod.config.js --progress",
    "prebuild:quiet": "npm run clean",
    "build:quiet": "cross-env NODE_ENV=production webpack --env.quiet -p --config webpack.prod.config.js --json > stats.json",
    "analyze": "webpack-bundle-analyzer -p 3001 stats.json dist/",
    "serve": "serve -l 8081",
    "mock": "json-server mock/db.json --port 3000 --delay 500 --read-only"
  },
